token_list = (
    'COMMENT',
    'ADD', 'SUB', 'MUL', 'DIV',
    'INTTYPE', 'BOOLTYPE', 'CHARTYPE', 'VOIDTYPE', 'VARTYPE',
    'INTL', 'BOOLL', 'CHARL',
    'LPAREN', 'RPAREN',
    'ID',
    'IF', 'ELSE', 'BREAK', 'CONTINUE', 'FOR', 'WHILE', 'RETURN',
    'BEQ', 'LEQ', 'EQ', 'NEQ',
)


class Tokens:
    pass

for t in token_list:
    Tokens.__dict__[t] = t

print(Tokens.COMMENT)

