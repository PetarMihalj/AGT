fn printpos(ptr, pos){
    out(ptr[pos]!);
    out('\n');
}

fn setpos(ptr, pos, val){
    ptr[pos]! = val;
}

fn main() -> i32{
    let H = heap_alloc<i32>(3);
    setpos(H, 1, 123);
    setpos(H, 2, 456);
    printpos(H, 0);
    printpos(H, 1);
    printpos(H, 2);
    heap_free(H);

    return 0;
}
